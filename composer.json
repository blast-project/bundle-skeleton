{
    "name": "blast-project/bundle-skeleton",
    "type": "symfony-bundle",
    "description": "Acme example bundle for Blast.",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "symfony/symfony": ">=3.2",
        "blast-project/core-bundle": ">=0.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": ">=3.2"
    },
    "license": "LGPL-3.0",
    "keywords": [],
    "homepage": "https://github.com/blast-project/bundle-skeleton",
    "authors": [
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Blast\\AcmeBundle\\": "src/",
            "Blast\\AcmeBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit-install": [
            "wget https://phar.phpunit.de/phpunit-5.7.phar --output-document=bin/phpunit",
            "chmod u+x bin/phpunit"
        ]
    },
    "version": "0.5.3"
}
